{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "rules": {
    "no-undef": "error",
    "react/jsx-no-undef": "error",
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  }
}